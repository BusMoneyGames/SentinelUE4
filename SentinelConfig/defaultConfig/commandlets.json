{  
   "commandlets":{  
      "_PkgInfoCommandlet":{  
         "command":"PkgInfoCommandlet",
         "detail_extract_types":[  
            "Blueprint",
            "StaticMesh"
         ],
         "flags":[  
            "names",
            "paths",
            "assetregistry"
         ],
         "detailed_extract":[  
            "paths",
            "depends",
            "imports",
            "exports",
            "names",
            "depends",
            "AssetRegistry"
         ]
      },
      "Build-Lighting":{  
         "command":"ResavePackages",
         "flags":[  
            "buildlighting",
            "BuildReflectionCaptures",
            "ForceEnableHLODForLevel",
            "ShouldBuildHLOD",
            "Map=/Game/Maps/Map_StartupTest",
            "SkipDeveloperFolders",
            "allowcommandletrendering",
            "ignorechangelist",
            "projectonly"
         ]
      },
      "Resave-All-Packages":{  
         "command":"ResavePackages",
         "flags":[  
            "ignorechangelist",
            "projectonly"
         ]
      },
      "Compile-Blueprints":{  
         "command":"CompileAllBlueprints",
         "flags":[  
            "projectonly"
         ]
      },
      "Resave-Blueprints":{  
         "command":"ResavePackages",
         "flags":[  
            "-IncludeChildClasses",
            "ResaveClass=Blueprint",
            "ignorechangelist",
            "projectonly"
         ]
      },
      "Resave-Levels":{  
         "command":"ResavePackages",
         "flags":[  
            "-IncludeChildClasses",
            "ResaveClass=World",
            "ignorechangelist",
            "projectonly"
         ]
      }
   }
}